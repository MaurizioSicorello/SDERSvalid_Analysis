# 1. Set up the loading matrix (LY)
LY_mat <- matrix(0, nrow = 24, ncol = 11)
colnames(LY_mat) <- c("g", paste0("s", 1:6), paste0("m", 1:4))
rownames(LY_mat) <- paste0("y", 1:24)
# g-factor loads on all indicators
LY_mat[, "g"] <- 0.3
# scale-specific factors
for (i in 1:6) {
LY_mat[((i - 1) * 4 + 1):(i * 4), paste0("s", i)] <- 0.5
}
# method factors
for (j in 1:4) {
LY_mat[seq(j, 24, by = 4), paste0("m", j)] <- 0.4
}
LY <- bind(LY_mat)  # ✅ convert to SimMatrix
# 2. Latent variable correlations (RPS)
RPS <- diag(1, 11)  # all latent variables orthogonal
RPS <- bind(RPS)    # ✅ convert to SimMatrix
# 3. Residual variances (RTE)
RTE <- diag(1 - rowSums(LY_mat^2))  # residuals so total var = 1
RTE <- bind(RTE)  # ✅ convert to SimMatrix
RTE
# Run a single simulation with 500 participants
simResult <- sim(1, n = 500, model = modelTrue)
