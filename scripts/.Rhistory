library("simsem")
install.packages("simsem")
library("lavaan")
# Create names for indicators
indicators <- paste0("y", 1:24)
# g-factor: loads on all indicators
g_factor <- paste("g =~", paste(indicators, collapse = " + "))
# Scale factors: each loads on 4 items
scale_factors <- sapply(1:6, function(i) {
idx <- ((i - 1) * 4 + 1):(i * 4)
items <- paste0("y", idx)
paste0("s", i, " =~ ", paste(items, collapse = " + "))
})
# Method factors: each loads on every 1st, 2nd, 3rd, or 4th item of each scale
method_factors <- sapply(1:4, function(m) {
items <- paste0("y", seq(m, 24, by = 4))
paste0("m", m, " =~ ", paste(items, collapse = " + "))
})
# Combine all parts of the model
model.syntax <- paste(c(g_factor, scale_factors, method_factors), collapse = "\n")
cat(model.syntax)
loadingValues <- matrix(NA, 24, 11)  # 1 g + 6 scales + 4 methods
colnames(loadingValues) <- c("g", paste0("s", 1:6), paste0("m", 1:4))
# Fill loadings
loadingValues[, "g"] <- 0.3  # g loads on all
for (i in 1:6) loadingValues[((i - 1) * 4 + 1):(i * 4), paste0("s", i)] <- 0.5
for (j in 1:4) loadingValues[seq(j, 24, by = 4), paste0("m", j)] <- 0.4
LY <- bind(loadingValues)
RPS <- diag(11)  # All latent variables orthogonal
RTE <- diag(0.51, 24)  # Residual variance, adjust as needed
modelTrue <- model(LY = LY, RPS = RPS, RTE = RTE, modelType = "CFA")
